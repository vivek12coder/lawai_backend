{
  "version": 2,
  "builds": [
    {
      "src": "app/main.py",
      "use": "@vercel/python"
    }
  ],
  "routes": [
    {
      "src": "/legal-qa",
      "methods": ["POST", "OPTIONS"],
      "dest": "app/main.py"
    },
    {
      "src": "/api/(.*)",
      "methods": ["GET", "POST", "OPTIONS"],
      "dest": "app/main.py"
    },
    {
      "src": "/(.*)",
      "dest": "app/main.py"
    }
  ],
  "env": {
    "PYTHONPATH": "."
  }
}